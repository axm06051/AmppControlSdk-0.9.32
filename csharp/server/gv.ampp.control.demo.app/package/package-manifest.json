{
  "name": "Sdk.DemoApp",
  "version": "1.0.0.3",
  "description": "A shell app for the creation of new ampp app team apps.",
  "summary": "The Demp App From the SDK",
  "title": "Sdk Demo App",
  "vru": 5,
  "tasks": [
    {
      "type": "config",
      "options": {
        "sourceFile": "templates\\appsettings-template.json",
        "destinationFile": "bin\\appsettings.json"
      }
    }
  ],
  "application": {
    "filePath": "bin/gv.ampp.control.demo.app",
    "arguments": "--port {{allocate_port}}",
    "workingFolder": "bin"
  },
  "clientDetails": {
    "grantType": "ImplicitAndClientCredentials",
    "scopes": [
      "platform",
      "cluster.readonly"
    ]
  },
  "applicationSettings": [
    {
      "name": "demoParameter",
      "label": "Demo Parameter",
      "description": "A Demo Parameter",
      "type": "Text",
      "defaultValue": "default parameter",
      "required": false
    }
  ],
  "packageOS": "LinuxX64"
}
